<script>
jQuery(document).ready(function ($) { 

  // Get the stationsList
    // Try and get it from the browser's local storage
    if (localStorage.getItem("bahnSharingStations5") === null) {
      // console.log('gotta get em all');
      // Set up our var to hold our stations
      // var stationsList = [{id:2,text:'duplicate'}];
      $.getJSON( "/stationsList.json", function(json) {
        //console.log( "station list download complete" );
        var stationsList = json; 
        // Save the station list to the browser's local storage for next time
        // http://htmldog.com/guides/javascript/advanced/localstorage/
        localStorage.setItem('bahnSharingStations5', JSON.stringify(stationsList));
        localStorage.removeItem('bahnSharingStations');
        localStorage.removeItem('bahnSharingStations1');
        localStorage.removeItem('bahnSharingStations2');
        localStorage.removeItem('bahnSharingStations3');
        renderSelect2(stationsList);
      })
        .done(function() {
        })
        .fail(function() {
          console.log( "station list retreival error!" );
        })
        .always(function() {
        });
    } else {
      var stationsList = JSON.parse(localStorage.getItem('bahnSharingStations5'));
      renderSelect2(stationsList);
    }

  // we need to manually call select2 so we can ensure the json
  // has been retrieved before it fires
  function renderSelect2(stationsList) {
    function displayCurrentValue(selectedObject, currentSearchTerm) {
      return currentSearchTerm;
    }

    $("#edit-departure-distance-origin-latlon.select2").select2({
      //placeholder: "Select a station",
      allowClear: true,
      minimumInputLength: 1,
      formatInputTooShort: function(term, minLength) {
        var rest = minLength - term.length;
        return 'Zum Start der Suche Tastatur drÃ¼cken';
      },
      nextSearchTerm: displayCurrentValue,
      //placeholder: "Anywhere",
      // search from the start of the string only
      matcher: function(term, text) { return text.toUpperCase().indexOf(term.toUpperCase())==0; },
      // Load in our station list
      data: stationsList,
      // Set "here" as the default
      initSelection: function(element, callback) {
        // Eventually we should querry the user's lat/lon and set this to their current location
        // or use a cookie to store their last city seached from
        var data = {id: '0,0,0,Stuttgart Hbf', text: 'City'};
        //console.log(data);
        callback(data);
      },
      // Add our 'needsclick' to each item, so FastClick doesn't get applied
      formatResult: function(result, container, query, escapeMarkup) {
        container.addClass('needsclick');
        return result.text;
      }
    }); 


    $("#edit-arrival-distance-origin-latlon.select2").select2({
      //placeholder: "Select a station",
      allowClear: true,
      minimumInputLength: 1,
      formatInputTooShort: function(term, minLength) {
        var rest = minLength - term.length;
        return 'Zum Start der Suche Tastatur drÃ¼cken';
      },
      nextSearchTerm: displayCurrentValue,
      //placeholder: "Anywhere",
      // search from the start of the string only
      matcher: function(term, text) { return text.toUpperCase().indexOf(term.toUpperCase())==0; },
      // Load in our station list
      data: stationsList,
      // Set "anywhere" as the default
      initSelection: function(element, callback) {
        // Eventually we should querry the user's lat/lon and set this to their current location
        // or use a cookie to store their last city seached from
        var data = {id: '0,0,0,Berlin Hbf', text: 'City'};
        callback(data);
      },
      // Add our 'needsclick' to each item, so FastClick doesn't get applied
      formatResult: function(result, container, query, escapeMarkup) {
        container.addClass('needsclick');
        return result.text;
      }
    }); 
  }

//  $(function() {
//    FastClick.attach(document.body);
//  });


  // The drupal module requires that lat/lon be entered in seperate fields
  // but we'd rather do it with one select option. This jquery bridges the gap.
  // We need the 4th value from the Json object to get the city name
  $('#edit-submit-train-stations').click(function() {
    $('#edit-departure-city').val(
      $('#edit-departure-distance-origin-latlon').val().split(',')[3]
    );

    $('#edit-arrival-city').val(
      $('#edit-arrival-distance-origin-latlon').val().split(',')[3]
    );
  });


});
</script>
    <link href="/sites/all/libraries/select2-3.4.2/select2.css?3" rel="stylesheet"/>
  </div>

  
  </div>
  </div>
</div>
</div>
</div>
    </section>

    
  </div>
</div>
