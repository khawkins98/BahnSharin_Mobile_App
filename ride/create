<script type="text/javascript">
  jQuery(document).ready(function ($) { 
    var now = new Date();
    var nowTime = new Date();
    var fill = 1;

    $('#edit-departuretime-value-datepicker-popup-0').mobiscroll().date({
        theme: 'android-ics light',
        lang: 'de',
        display: 'modal',
        mode: 'scroller',
        headerText: false,
        minDate: new Date(now.getFullYear(), now.getMonth(), now.getDate()),
        dateFormat: 'd.m.yy',
        dateOrder: 'ddmmyy',
        //minDate: new Date(now.getFullYear(), now.getMonth(), now.getDate()),
        //invalid: [ 'w0', 'w6', '5/1', '12/24', '12/25' ]
    });   
    $('#edit-departuretime-value-datepicker-popup-0').mobiscroll('setDate', now, fill, nowTime);

    $('#edit-departuretime-value-timeEntry-popup-1').mobiscroll().time({
        theme: 'android-ics light',
        lang: 'de',
        display: 'modal',
        mode: 'scroller',
    //    setText: false,
    //    cancelText: false,
        headerText: false
    });   

    // Put it in the right YYYY-MM-DD format on submit
    $('#views-exposed-form-train-stations-page-4').submit(function( event ) {
      var mobiDate = $('#edit-departuretime-value-datepicker-popup-0').mobiscroll('getDate');
      var date = new Date(mobiDate);
      var day = date.getDate();
      var month = date.getMonth()+1;
      var year = date.getFullYear();
      //alert(date+' - '+year+'-'+month+'-'+day);
      $("#edit-departuretime-value-datepicker-popup-0").val(year+'-'+month+'-'+day);
    });

    // Seat input
    $('#edit-seats').mobiscroll().select({
        theme: 'android-ics light',
        lang: 'de',
        display: 'modal',
        mode: 'scroller',
        inputClass: 'i-txt',
        width: 200
    });
/*
    // City lists    
    $('#edit-departure-distance-origin-latlon').mobiscroll().select({
